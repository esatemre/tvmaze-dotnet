      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: /Users/esatemre/.microsoft/usersecrets
      target: /root/.microsoft/usersecrets
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: /Users/esatemre/.nuget/packages/
      target: /root/.nuget/packages
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: /Users/esatemre/Projects/Tvmaze
      target: /src
      bind:
        create_host_path: true
