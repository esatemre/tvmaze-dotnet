name: dockercompose549299049324158017
services:
  tvmaze.api:
    build:
      context: /Users/esatemre/Projects/Tvmaze
      dockerfile: Tvmaze.Api/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: https://+:443;http://+:80
    image: tvmaze
    networks:
      default: null
    ports:
    - mode: ingress
      target: 80
      protocol: tcp
    - mode: ingress
      target: 443
      protocol: tcp
    volumes:
    - type: bind
      source: /Users/esatemre/.aspnet/https
      target: /root/.aspnet/https
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: /Users/esatemre/.microsoft/usersecrets
      target: /root/.microsoft/usersecrets
      read_only: true
      bind:
        create_host_path: true
  tvmaze.scraper:
    build:
      context: /Users/esatemre/Projects/Tvmaze
      dockerfile: Tvmaze.Scraper/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: https://+:443;http://+:80
    image: tvmaze
    networks:
      default: null
    ports:
    - mode: ingress
      target: 80
      protocol: tcp
    - mode: ingress
      target: 443
      protocol: tcp
    volumes:
    - type: bind
      source: /Users/esatemre/.aspnet/https
      target: /root/.aspnet/https
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: /Users/esatemre/.microsoft/usersecrets
      target: /root/.microsoft/usersecrets
      read_only: true
      bind:
        create_host_path: true
  tvmze.postgres:
    container_name: postgres_container
    environment:
      PGDATA: /data/postgres
      POSTGRES_PASSWORD: mysecretpassword
      POSTGRES_USER: postgres
    image: postgres
    networks:
      default: null
    ports:
    - mode: ingress
      target: 5432
      published: "5432"
      protocol: tcp
    restart: unless-stopped
    volumes:
    - type: volume
      source: postgres
      target: /data/postgres
      volume: {}
networks:
  default:
    name: dockercompose549299049324158017_default
volumes:
  postgres:
    name: dockercompose549299049324158017_postgres